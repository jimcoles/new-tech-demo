<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="WebApp_ID" version="2.5">

    <display-name>JBoss WAR for New Tech Demo App</display-name>

    <!--
        Resteasy servlet config switches:
            https://docs.jboss.org/resteasy/docs/3.6.1.Final/userguide/html_single/#configuration_switches

        <context-param>
            <param-name>resteasy.servlet.mapping.prefix</param-name>
            <param-value>/jasmin</param-value>
        </context-param>
    -->

    <context-param>
        <param-name>org.jboss.weld.development</param-name>
        <param-value>true</param-value>
    </context-param>

    <session-config>
        <session-timeout>10</session-timeout>
    </session-config>

</web-app>